<script>
    var deleteLog = false;
    //	Pagepiling js
    //=====================================================
    // AJAX POST NEWSLETTER !!
    function ajaxPost(url, data, callback) {
        let req = new XMLHttpRequest();
        req.open("POST", url);
        req.addEventListener("load", function () {
            if (req.status >= 200 && req.status < 400) {
                // Appelle la fonction callback en lui passant la réponse de la requête
                callback(req.responseText);
            } else {
                console.error(req.status + " " + req.statusText + " " + url);
            }
        });
        req.addEventListener("error", function () {
            console.error("Erreur réseau avec l'URL " + url);
        });
        req.send(data);
    }



    let formNewsLetterMentionsLegales= document.getElementById('formNewsLetterMentionsLegales');
    formNewsLetterMentionsLegales.addEventListener('click', function () {
        let contentNewsLetterMentionsLegales = document.getElementById('contentNewsLetterMentionsLegales');

        let email = contentNewsLetterMentionsLegales.value;
        ajaxPost("{{ path('postNewsletter') }}", email, function() {

            formNewsLetterMentionsLegales.value = "Inscrit(e) !";
        })
    })

