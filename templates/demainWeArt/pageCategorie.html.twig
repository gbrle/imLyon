<script>
    var deleteLog = false;
    //	Pagepiling js
    //=====================================================

    // AJAX POST NEWSLETTER !!
    function ajaxPost(url, data, callback) {
        let req = new XMLHttpRequest();
        req.open("POST", url);
        req.addEventListener("load", function () {
            if (req.status >= 200 && req.status < 400) {
                // Appelle la fonction callback en lui passant la réponse de la requête
                callback(req.responseText);
            } else {
                console.error(req.status + " " + req.statusText + " " + url);
            }
        });
        req.addEventListener("error", function () {
            console.error("Erreur réseau avec l'URL " + url);
        });
        req.send(data);
    }



    let formNewsLetterCategorie = document.getElementById('formNewsLetterCategorie');
    formNewsLetterCategorie.addEventListener('click', function () {
        let contentNewsLetterCategorie = document.getElementById('contentNewsLetterCategorie');

        let email = contentNewsLetterCategorie.value;
        ajaxPost("{{ path('postNewsletter') }}", email, function() {

            formNewsLetterCategorie.value = "Inscrit(e) !";
        })
    })
    // AJAX POST NEWSLETTER !! END
    //	Sidebar Nav Menu js
    //=====================================================
    function openNav() {
        document.getElementById("mysidenav").style.width = "100%";
    }
    function closeNav() {
        document.getElementById("mysidenav").style.width = "0";
    }
</script>
